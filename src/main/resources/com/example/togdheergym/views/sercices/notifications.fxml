<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<TabPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.togdheergym.controllers.NotificationsController">
   <tabs>
      <Tab text="Daily Report">
         <content>
            <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="492.0" prefWidth="705.0" style="-fx-background-color: #DFE0DA;">
               <children>
                  <TableView fx:id="tableView" layoutX="13.0" layoutY="47.0" prefHeight="431.0" prefWidth="679.0">
                    <columns>
                      <TableColumn fx:id="costumerId" prefWidth="75.0" text="costumer id" />
                      <TableColumn fx:id="fname" prefWidth="95.0" text="first name" />
                        <TableColumn fx:id="lname" prefWidth="116.0" text="last name" />
                        <TableColumn fx:id="phone" prefWidth="116.0" text="phone" />
                        <TableColumn fx:id="datejoined" prefWidth="116.0" text="join date" />
                        <TableColumn fx:id="expDate" prefWidth="116.0" text="exp date" />
                    </columns>
                  </TableView>
                  <HBox alignment="CENTER_LEFT" prefHeight="38.0" prefWidth="705.0" spacing="530.0" style="-fx-background-color: #132332;">
                     <children>
                        <Label prefHeight="17.0" prefWidth="124.0" text="Daily report" textFill="#fcebeb">
                           <font>
                              <Font name="Arial Black" size="12.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </Label>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../icons/icons8_cancel_50px.png" />
                           </image>
                        </ImageView>
                     </children>
                  </HBox>
               </children>
               <effect>
                  <DropShadow />
               </effect>
            </AnchorPane>
         </content>
      </Tab>
      <Tab text="Monthly Report">
         <content>
            <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="492.0" prefWidth="705.0" style="-fx-background-color: #DFE0DA;">
